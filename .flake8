[flake8]
exclude =
    venv,
    .venv,
    env,
    .git,
    __pycache__,
    build,
    dist,
    staticfiles,
    media,
    */migrations/*,
    csv_processor/views copy.py,
    csv_processor/urls copy.py
max-line-length = 120
extend-select = E9,F63,F7,F82
per-file-ignores =
    csv_processor/tests/*.py:E501